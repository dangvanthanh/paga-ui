{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"strictNullChecks": true,
		"jsx": "preserve",
		"jsxImportSource": "react-jsx",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/styled-system/*": ["./styled-system/*"]
		}
	},
	"include": [".astro/types.d.ts", "styled-system", "**/*"],
	"exclude": ["dist"]
}
