{
  "name": "@paga/preset",
  "private": true,
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --format=esm,cjs",
    "lint": "pnpm dlx @biomejs/biome lint ./src/*/*.ts --apply",
    "format": "pnpm dlx @biomejs/biome format ./src/**/*.ts --write"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@pandacss/dev": "^0.32.1",
    "@pandacss/types": "^0.32.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}