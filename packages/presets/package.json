{
  "name": "@paga/presets",
  "private": true,
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --format=esm,cjs",
    "format": "bun run biome format ./src/**/*.ts --write"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.1",
    "@pandacss/dev": "^0.15.0",
    "@pandacss/types": "^0.15.0",
    "bun-types": "^1.0.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}