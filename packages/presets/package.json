{
  "name": "@paga/presets",
  "private": true,
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --format=esm,cjs",
    "format": "bun run biome format ./src/**/*.ts --write"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@pandacss/dev": "^0.18.3",
    "@pandacss/types": "^0.18.3",
    "bun-types": "^1.0.14",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}